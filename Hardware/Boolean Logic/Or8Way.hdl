/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a= in[0], b= in[1], out= tempA1);
    Or(a= in[2], b= in[3], out= tempA2);
    Or(a= in[4], b= in[5], out= tempA3);
    Or(a= in[6], b= in[7], out= tempA4);
    Or(a= tempA1, b= tempA2, out= tempB1);
    Or(a= tempA3, b= tempA4, out= tempB2);
    Or(a= tempB1, b= tempB2, out= out);
}